{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      },
      {
        "src": "backend/server/route.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/server/route.py"
      },
      {
        "src": "/(.*)",
        "dest": "/.next/$1"
      }
    ]
  }
  